{"ast":null,"code":"// import axios from \"axios\";\n// import moment from \"moment\";\n// const URL = \"http://localhost:3003\";\nimport ProviderComponent from \"./ProviderComponent.vue\";\nimport OrdersComponent from \"./OrdersComponent.vue\";\nexport default {\n  components: {\n    ProviderComponent,\n    OrdersComponent\n  },\n\n  data() {\n    return {\n      // ordData: [],\n      // orderStatus: [\n      //   \"Pendente de confirmação\",\n      //   \"Pedido confirmado\",\n      //   \"Não reconhece o pedido\",\n      //   \"Mercadoria não recebida\",\n      //   \"Recebida com avaria\",\n      //   \"Devolvida\",\n      //   \"Recebida com devolução parcial\",\n      //   \"Recebida e confirmada\",\n      //   \"Pagamento Autorizado\",\n      // ],\n      // buyData: [],\n      // provData: [],\n      handGreen: this.getImgUrl(\"handgreen.png\"),\n      handWhite: this.getImgUrl(\"handwhite.png\"),\n      icon: this.getImgUrl(\"handgreen.png\"),\n      showComponent: false\n    };\n  },\n\n  async created() {\n    const getOrders = await axios.get(`${URL}/orders`);\n    this.ordData = getOrders.data;\n    const getBuyers = await axios.get(`${URL}/buyers/`);\n    this.buyData = getBuyers.data;\n    const getProviders = await axios.get(`${URL}/providers/`);\n    this.provData = getProviders.data;\n  },\n\n  methods: {\n    format_date(value) {\n      if (value) {\n        const newData = moment(String(value)).format(\"DD/MM/YYYY\");\n        return newData;\n      }\n    },\n\n    find_buyer(id) {\n      return this.buyData.filter(buyer => buyer.id === id)[0].name;\n    },\n\n    find_provider(id) {\n      return this.provData.filter(provider => provider.id === id)[0].name;\n    },\n\n    getImgUrl(pic) {\n      return require(\"../assets/\" + pic);\n    }\n\n  },\n  computed: {\n    allOrd() {\n      return this.ordData.map(ord => ({ ...ord,\n        emissionDate: this.format_date(ord.emissionDate),\n        value: parseInt(ord.value).toLocaleString(\"pt-br\", {\n          style: \"currency\",\n          currency: \"BRL\"\n        }),\n        orderStatusBuyer: this.orderStatus[parseInt(ord.orderStatusBuyer)],\n        buyerId: this.find_buyer(ord.buyerId),\n        providerId: this.find_provider(ord.providerId)\n      }));\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAwEA;AACA;AAEA;AAEA,OAAOA,iBAAP,MAA8B,yBAA9B;AACA,OAAOC,eAAP,MAA4B,uBAA5B;AAEA,eAAe;EACbC,UAAU,EAAE;IACVF,iBADU;IAEVC;EAFU,CADC;;EAKbE,IAAI,GAAG;IACL,OAAO;MACL;MACA;MACA;MACA;MACA;MACA;MACA;MACA;MACA;MACA;MACA;MACA;MACA;MACA;MACAC,SAAS,EAAE,KAAKC,SAAL,CAAe,eAAf,CAfN;MAgBLC,SAAS,EAAE,KAAKD,SAAL,CAAe,eAAf,CAhBN;MAiBLE,IAAI,EAAE,KAAKF,SAAL,CAAe,eAAf,CAjBD;MAkBLG,aAAa,EAAE;IAlBV,CAAP;EAoBD,CA1BY;;EA2Bb,MAAMC,OAAN,GAAgB;IACd,MAAMC,SAAQ,GAAI,MAAMC,KAAK,CAACC,GAAN,CAAW,GAAEC,GAAI,SAAjB,CAAxB;IACA,KAAKC,OAAL,GAAeJ,SAAS,CAACP,IAAzB;IACA,MAAMY,SAAQ,GAAI,MAAMJ,KAAK,CAACC,GAAN,CAAW,GAAEC,GAAI,UAAjB,CAAxB;IACA,KAAKG,OAAL,GAAeD,SAAS,CAACZ,IAAzB;IACA,MAAMc,YAAW,GAAI,MAAMN,KAAK,CAACC,GAAN,CAAW,GAAEC,GAAI,aAAjB,CAA3B;IACA,KAAKK,QAAL,GAAgBD,YAAY,CAACd,IAA7B;EACD,CAlCY;;EAmCbgB,OAAO,EAAE;IACPC,WAAW,CAACC,KAAD,EAAQ;MACjB,IAAIA,KAAJ,EAAW;QACT,MAAMC,OAAM,GAAIC,MAAM,CAACC,MAAM,CAACH,KAAD,CAAP,CAAN,CAAsBI,MAAtB,CAA6B,YAA7B,CAAhB;QACA,OAAOH,OAAP;MACF;IACD,CANM;;IAQPI,UAAU,CAACC,EAAD,EAAK;MACb,OAAO,KAAKX,OAAL,CAAaY,MAAb,CAAqBC,KAAD,IAAWA,KAAK,CAACF,EAAN,KAAaA,EAA5C,EAAgD,CAAhD,EAAmDG,IAA1D;IACD,CAVM;;IAYPC,aAAa,CAACJ,EAAD,EAAK;MAChB,OAAO,KAAKT,QAAL,CAAcU,MAAd,CAAsBI,QAAD,IAAcA,QAAQ,CAACL,EAAT,KAAgBA,EAAnD,EAAuD,CAAvD,EAA0DG,IAAjE;IACD,CAdM;;IAgBPzB,SAAS,CAAC4B,GAAD,EAAM;MACb,OAAOC,OAAO,CAAC,eAAeD,GAAhB,CAAd;IACD;;EAlBM,CAnCI;EAuDbE,QAAQ,EAAE;IACRC,MAAM,GAAG;MACP,OAAO,KAAKtB,OAAL,CAAauB,GAAb,CAAkBC,GAAD,KAAU,EAChC,GAAGA,GAD6B;QAEhCC,YAAY,EAAE,KAAKnB,WAAL,CAAiBkB,GAAG,CAACC,YAArB,CAFkB;QAGhClB,KAAK,EAAEmB,QAAQ,CAACF,GAAG,CAACjB,KAAL,CAAR,CAAoBoB,cAApB,CAAmC,OAAnC,EAA4C;UACjDC,KAAK,EAAE,UAD0C;UAEjDC,QAAQ,EAAE;QAFuC,CAA5C,CAHyB;QAOhCC,gBAAgB,EAAE,KAAKC,WAAL,CAAiBL,QAAQ,CAACF,GAAG