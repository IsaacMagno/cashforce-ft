{"ast":null,"code":"import axios from \"axios\";\nimport moment from \"moment\";\nimport ProviderComponent from \"./ProviderComponent\";\nimport OrdersComponent from \"./OrdersComponent\";\nconst URL = \"http://localhost:3003\";\nexport default {\n  components: {\n    ProviderComponent\n  },\n\n  data() {\n    return {\n      ordData: [],\n      orderStatus: [\"Pendente de confirmação\", \"Pedido confirmado\", \"Não reconhece o pedido\", \"Mercadoria não recebida\", \"Recebida com avaria\", \"Devolvida\", \"Recebida com devolução parcial\", \"Recebida e confirmada\", \"Pagamento Autorizado\"],\n      buyData: [],\n      provData: [],\n      handGreen: this.getImgUrl(\"handgreen.png\"),\n      handWhite: this.getImgUrl(\"handwhite.png\"),\n      icon: this.getImgUrl(\"handgreen.png\"),\n      showComponent: false\n    };\n  },\n\n  async created() {\n    const getOrders = await axios.get(`${URL}/orders`);\n    this.ordData = getOrders.data;\n    const getBuyers = await axios.get(`${URL}/buyers/`);\n    this.buyData = getBuyers.data;\n    const getProviders = await axios.get(`${URL}/providers/`);\n    this.provData = getProviders.data;\n  },\n\n  methods: {\n    format_date(value) {\n      if (value) {\n        const newData = moment(String(value)).format(\"DD/MM/YYYY\");\n        return newData;\n      }\n    },\n\n    find_buyer(id) {\n      return this.buyData.filter(buyer => buyer.id === id)[0].name;\n    },\n\n    find_provider(id) {\n      return this.provData.filter(provider => provider.id === id)[0].name;\n    },\n\n    getImgUrl(pic) {\n      return require(\"../assets/\" + pic);\n    }\n\n  },\n  computed: {\n    allOrd() {\n      return this.ordData.map(ord => ({ ...ord,\n        emissionDate: this.format_date(ord.emissionDate),\n        value: parseInt(ord.value).toLocaleString(\"pt-br\", {\n          style: \"currency\",\n          currency: \"BRL\"\n        }),\n        orderStatusBuyer: this.orderStatus[parseInt(ord.orderStatusBuyer)],\n        buyerId: this.find_buyer(ord.buyerId),\n        providerId: this.find_provider(ord.providerId)\n      }));\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAuEA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,MAAP,MAAmB,QAAnB;AAEA,OAAOC,iBAAP,MAA8B,qBAA9B;AACA,OAAOC,eAAP,MAA4B,mBAA5B;AAEA,MAAMC,GAAE,GAAI,uBAAZ;AAEA,eAAe;EACbC,UAAU,EAAE;IACVH;EADU,CADC;;EAIbI,IAAI,GAAG;IACL,OAAO;MACLC,OAAO,EAAE,EADJ;MAELC,WAAW,EAAE,CACX,yBADW,EAEX,mBAFW,EAGX,wBAHW,EAIX,yBAJW,EAKX,qBALW,EAMX,WANW,EAOX,gCAPW,EAQX,uBARW,EASX,sBATW,CAFR;MAaLC,OAAO,EAAE,EAbJ;MAcLC,QAAQ,EAAE,EAdL;MAeLC,SAAS,EAAE,KAAKC,SAAL,CAAe,eAAf,CAfN;MAgBLC,SAAS,EAAE,KAAKD,SAAL,CAAe,eAAf,CAhBN;MAiBLE,IAAI,EAAE,KAAKF,SAAL,CAAe,eAAf,CAjBD;MAkBLG,aAAa,EAAE;IAlBV,CAAP;EAoBD,CAzBY;;EA0Bb,MAAMC,OAAN,GAAgB;IACd,MAAMC,SAAQ,GAAI,MAAMjB,KAAK,CAACkB,GAAN,CAAW,GAAEd,GAAI,SAAjB,CAAxB;IACA,KAAKG,OAAL,GAAeU,SAAS,CAACX,IAAzB;IACA,MAAMa,SAAQ,GAAI,MAAMnB,KAAK,CAACkB,GAAN,CAAW,GAAEd,GAAI,UAAjB,CAAxB;IACA,KAAKK,OAAL,GAAeU,SAAS,CAACb,IAAzB;IACA,MAAMc,YAAW,GAAI,MAAMpB,KAAK,CAACkB,GAAN,CAAW,GAAEd,GAAI,aAAjB,CAA3B;IACA,KAAKM,QAAL,GAAgBU,YAAY,CAACd,IAA7B;EACD,CAjCY;;EAkCbe,OAAO,EAAE;IACPC,WAAW,CAACC,KAAD,EAAQ;MACjB,IAAIA,KAAJ,EAAW;QACT,MAAMC,OAAM,GAAIvB,MAAM,CAACwB,MAAM,CAACF,KAAD,CAAP,CAAN,CAAsBG,MAAtB,CAA6B,YAA7B,CAAhB;QACA,OAAOF,OAAP;MACF;IACD,CANM;;IAQPG,UAAU,CAACC,EAAD,EAAK;MACb,OAAO,KAAKnB,OAAL,CAAaoB,MAAb,CAAqBC,KAAD,IAAWA,KAAK,CAACF,EAAN,KAAaA,EAA5C,EAAgD,CAAhD,EAAmDG,IAA1D;IACD,CAVM;;IAYPC,aAAa,CAACJ,EAAD,EAAK;MAChB,OAAO,KAAKlB,QAAL,CAAcmB,MAAd,CAAsBI,QAAD,IAAcA,QAAQ,CAACL,EAAT,KAAgBA,EAAnD,EAAuD,CAAvD,EAA0DG,IAAjE;IACD,CAdM;;IAgBPnB,SAAS,CAACsB,GAAD,EAAM;MACb,OAAOC,OAAO,CAAC,eAAeD,GAAhB,CAAd;IACD;;EAlBM,CAlCI;EAsDbE,QAAQ,EAAE;IACRC,MAAM,GAAG;MACP,OAAO,KAAK9B,OAAL,CAAa+B,GAAb,CAAkBC,GAAD,KAAU,EAChC,GAAGA,GAD6B;QAEhCC,YAAY,EAAE,KAAKlB,WAAL,CAAiBiB,GAAG,CAACC,YAArB,CAFkB;QAGhCjB,KAAK,EAAEkB,QAAQ,CAACF,GAAG,CAAChB,KAAL,CAAR,CAAoBmB,cAApB,CAAmC,OAAnC,EAA4C;UACjDC,KAAK,EAAE,UAD0C;UAEjDC,QAAQ,EAAE;QAFuC,CAA5C,CAHyB;QAOhCC,gBAAgB,EAAE,KAAKrC,WAAL,CAAiBiC,QAAQ,CAACF,GAAG,CAACM,gBAAL,CAAzB,CAPc;QAQhCC,OAAO,EAAE,KAAKnB,UAAL,CAAgBY,GAAG,CAACO,OAApB,CARuB;QAShCC,UAAU,EAAE,KAAKf,aAAL,CAAmBO,GAAG,CAACQ,UAAvB;MAToB,CAAV,CAAjB,CAAP;IAWD;;EAbO;AAtDG,CAAf","names":["axios","moment","ProviderComponent","OrdersComponent","URL","components","data","ordData","orderStatus","buyData","provData","handGreen","getImgUrl","handWhite","icon","showComponent","created","getOrders","get","getBuyers","getProviders","methods","format_date","value","newData","String","format","find_buyer","id","filter","buyer","name","find_provider","provider","pic","require","computed","allOrd","map","ord","emissionDate","parseInt","toLocaleString","style","currency","orderStatusBuyer","buyerId","providerId"],"sourceRoot":"","sources":["/home/isaac/myProjects/cashforce/cashforce-ft/src/components/OrdersPage.vue"],"sourcesContent":["<template>\n  <div class=\"container-fluid mt-4 p-5\" id=\"root\">\n    <div class=\"row\">\n      <div class=\"col-2 mb-5\">\n        <img :src=\"getImgUrl('logo-cashforce.png')\" alt=\"Cashforce logo\" />\n      </div>\n    </div>\n    <div class=\"row\">\n      <div class=\"col-2\">\n        <button\n          id=\"btn-invoice\"\n          v-on:mouseover=\"icon = handWhite\"\n          v-on:mouseout=\"icon = handGreen\"\n          type=\"button\"\n          v-on:click=\"showComponent = !showComponent\"\n        >\n          <img :src=\"icon\" id=\"handGreen\" />\n          Notas fiscais\n        </button>\n      </div>\n      <div class=\"col\">\n        <div class=\"container card p-5\" v-if=\"showComponent\">\n          <div class=\"row mb-3\">\n            <h3 class=\"fw-bold\">\n              <img :src=\"getImgUrl('blueHands.png')\" id=\"blueHands\" />\n              Notas Fiscais\n            </h3>\n            <p id=\"p-head\">Visualize as notas fiscais que você tem.</p>\n          </div>\n          <div class=\"row\">\n            <table class=\"table-sm\">\n              <thead>\n                <tr class=\"spaceUnder\">\n                  <th scope=\"col\">NOTA FISCAL</th>\n                  <th scope=\"col\">SACADO</th>\n                  <th scope=\"col\">CEDENTE</th>\n                  <th scope=\"col\">EMISSÃO</th>\n                  <th scope=\"col\">VALOR</th>\n                  <th scope=\"col\">STATUS</th>\n                </tr>\n              </thead>\n              <tbody>\n                <tr v-for=\"(d, i) in allOrd\" v-bind:key=\"i\" class=\"spaceUnder\">\n                  <td scope=\"row\">{{ d.orderNfId }}</td>\n                  <td>{{ d.buyerId }}</td>\n                  <td>{{ d.providerId }}</td>\n                  <td>{{ d.emissionDate }}</td>\n                  <td class=\"ordVal fw-bold\">{{ d.value }}</td>\n                  <td class=\"ordVal fw-bold\">{{ d.orderStatusBuyer }}</td>\n                  <td>\n                    <button\n                      type=\"button\"\n                      class=\"btn btn-outline-secondary btn-sm\"\n                      id=\"provider-data\"\n                    >\n                      Dados do Cedente\n                    </button>\n                  </td>\n                </tr>\n              </tbody>\n            </table>\n          </div>\n          <ProviderComponent />\n        </div>\n        <div v-else></div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport moment from \"moment\";\n\nimport ProviderComponent from \"./ProviderComponent\";\nimport OrdersComponent from \"./OrdersComponent\";\n\nconst URL = \"http://localhost:3003\";\n\nexport default {\n  components: {\n    ProviderComponent,\n  },\n  data() {\n    return {\n      ordData: [],\n      orderStatus: [\n        \"Pendente de confirmação\",\n        \"Pedido confirmado\",\n        \"Não reconhece o pedido\",\n        \"Mercadoria não recebida\",\n        \"Recebida com avaria\",\n        \"Devolvida\",\n        \"Recebida com devolução parcial\",\n        \"Recebida e confirmada\",\n        \"Pagamento Autorizado\",\n      ],\n      buyData: [],\n      provData: [],\n      handGreen: this.getImgUrl(\"handgreen.png\"),\n      handWhite: this.getImgUrl(\"handwhite.png\"),\n      icon: this.getImgUrl(\"handgreen.png\"),\n      showComponent: false,\n    };\n  },\n  async created() {\n    const getOrders = await axios.get(`${URL}/orders`);\n    this.ordData = getOrders.data;\n    const getBuyers = await axios.get(`${URL}/buyers/`);\n    this.buyData = getBuyers.data;\n    const getProviders = await axios.get(`${URL}/providers/`);\n    this.provData = getProviders.data;\n  },\n  methods: {\n    format_date(value) {\n      if (value) {\n        const newData = moment(String(value)).format(\"DD/MM/YYYY\");\n        return newData;\n      }\n    },\n\n    find_buyer(id) {\n      return this.buyData.filter((buyer) => buyer.id === id)[0].name;\n    },\n\n    find_provider(id) {\n      return this.provData.filter((provider) => provider.id === id)[0].name;\n    },\n\n    getImgUrl(pic) {\n      return require(\"../assets/\" + pic);\n    },\n  },\n  computed: {\n    allOrd() {\n      return this.ordData.map((ord) => ({\n        ...ord,\n        emissionDate: this.format_date(ord.emissionDate),\n        value: parseInt(ord.value).toLocaleString(\"pt-br\", {\n          style: \"currency\",\n          currency: \"BRL\",\n        }),\n        orderStatusBuyer: this.orderStatus[parseInt(ord.orderStatusBuyer)],\n        buyerId: this.find_buyer(ord.buyerId),\n        providerId: this.find_provider(ord.providerId),\n      }));\n    },\n  },\n};\n</script>\n\n<style>\nbody {\n  background-color: #fbfdfe;\n}\n\nh3 {\n  color: #021b51;\n}\n\nth {\n  color: #a1a8b8;\n  font-size: 13px;\n}\n\ntd {\n  color: #4d5566;\n  font-size: 14px;\n}\n\ntr.spaceUnder > th {\n  padding-bottom: 1em;\n}\n\ntr.spaceUnder > td {\n  padding-bottom: 1em;\n}\n\n#p-head {\n  color: #727d94;\n  font-size: 15px;\n}\n\n#blueHands {\n  width: 33px;\n  margin-right: 10px;\n}\n\n#btn-invoice {\n  border-radius: 12px;\n  width: 200px;\n  height: 45px;\n  font-size: 15px;\n  cursor: pointer;\n  border: none;\n  outline: none;\n  background: transparent;\n  color: #00ad8c;\n  position: relative;\n  transition: all 0.3s;\n  z-index: 1;\n}\n\n#btn-invoice::before {\n  border-radius: 12px;\n  content: \"\";\n  position: absolute;\n  top: 0;\n  left: 0;\n  width: 5px;\n  height: 100%;\n  background-color: #00ad8c;\n  z-index: -1;\n  transition: all 0.3s;\n}\n\n#btn-invoice:hover::before {\n  width: 100%;\n}\n\n#btn-invoice:hover {\n  color: #ffffff;\n}\n\n#btn-invoice:active:before {\n  background: #b9b9b9;\n}\n\n#handGreen {\n  width: 20px;\n  margin-right: 15px;\n}\n\n#provider-data {\n  border-radius: 16px;\n  padding-left: 25px;\n  padding-right: 25px;\n}\n\n.ordVal {\n  color: #00ad8c;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}