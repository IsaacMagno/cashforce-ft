{"ast":null,"code":"import axios from \"axios\";\nimport moment from \"moment\";\nexport default {\n  data() {\n    return {\n      ordData: []\n    };\n  },\n\n  async created() {\n    const getData = await axios.get(\"http://localhost:3003/orders\");\n    this.ordData = getData.data;\n  },\n\n  methods: {\n    format_date(value) {\n      if (value) {\n        const newData = moment(String(value)).format(\"L\");\n        return newData;\n      }\n    }\n\n  },\n  computed: {\n    allData() {\n      return this.ordData.map(ord => ({ ...ord,\n        emissionDate: this.format_date(ord.emissionDate),\n        value: parseInt(ord.value).toLocaleString(\"pt-br\", {\n          style: \"currency\",\n          currency: \"BRL\"\n        })\n      }));\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAoDA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,MAAP,MAAmB,QAAnB;AAEA,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLC,OAAO,EAAE;IADJ,CAAP;EAGD,CALY;;EAMb,MAAMC,OAAN,GAAgB;IACd,MAAMC,OAAM,GAAI,MAAML,KAAK,CAACM,GAAN,CAAU,8BAAV,CAAtB;IACA,KAAKH,OAAL,GAAeE,OAAO,CAACH,IAAvB;EACD,CATY;;EAUbK,OAAO,EAAE;IACPC,WAAW,CAACC,KAAD,EAAQ;MACjB,IAAIA,KAAJ,EAAW;QACT,MAAMC,OAAM,GAAIT,MAAM,CAACU,MAAM,CAACF,KAAD,CAAP,CAAN,CAAsBG,MAAtB,CAA6B,GAA7B,CAAhB;QACA,OAAOF,OAAP;MACF;IACD;;EANM,CAVI;EAkBbG,QAAQ,EAAE;IACRC,OAAO,GAAG;MACR,OAAO,KAAKX,OAAL,CAAaY,GAAb,CAAkBC,GAAD,KAAU,EAChC,GAAGA,GAD6B;QAEhCC,YAAY,EAAE,KAAKT,WAAL,CAAiBQ,GAAG,CAACC,YAArB,CAFkB;QAGhCR,KAAK,EAAES,QAAQ,CAACF,GAAG,CAACP,KAAL,CAAR,CAAoBU,cAApB,CAAmC,OAAnC,EAA4C;UACjDC,KAAK,EAAE,UAD0C;UAEjDC,QAAQ,EAAE;QAFuC,CAA5C;MAHyB,CAAV,CAAjB,CAAP;IAQD;;EAVO;AAlBG,CAAf","names":["axios","moment","data","ordData","created","getData","get","methods","format_date","value","newData","String","format","computed","allData","map","ord","emissionDate","parseInt","toLocaleString","style","currency"],"sourceRoot":"","sources":["/home/isaac/myProjects/cashforce/cashforce-ft/src/components/OrdersPage.vue"],"sourcesContent":["<template>\n  <div class=\"container-fluid\">\n    <div class=\"row\">\n      <div class=\"col-2\">\n        <p>Logotipo</p>\n        <button class=\"btn btn-sn bg-primary\">notasFiscais</button>\n      </div>\n      <div class=\"col\">\n        <div class=\"container card\">\n          <div class=\"row\">\n            <h3>Notas Fiscais</h3>\n            <p>Visualize as notas fiscais que você tem.</p>\n          </div>\n          <div class=\"row\">\n            <table class=\"table\">\n              <thead>\n                <tr>\n                  <th scope=\"col\">NOTA FISCAL</th>\n                  <th scope=\"col\">SACADO</th>\n                  <th scope=\"col\">CEDENTE</th>\n                  <th scope=\"col\">EMISSÃO</th>\n                  <th scope=\"col\">VALOR</th>\n                  <th scope=\"col\">STATUS</th>\n                </tr>\n              </thead>\n              <tbody>\n                <tr v-for=\"(d, i) in allData\" v-bind:key=\"i\">\n                  <td>{{ d.orderNfId }}</td>\n                  <td>SACADO 001</td>\n                  <td>CEDENTE 002</td>\n                  <td>{{ d.emissionDate }}</td>\n                  <td>{{ d.value }}</td>\n                  <td>RECEBIDO</td>\n                  <td>\n                    <button\n                      type=\"button\"\n                      class=\"btn btn-outline-secondary btn-sm\"\n                    >\n                      Dados do Cedente\n                    </button>\n                  </td>\n                </tr>\n              </tbody>\n            </table>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport moment from \"moment\";\n\nexport default {\n  data() {\n    return {\n      ordData: [],\n    };\n  },\n  async created() {\n    const getData = await axios.get(\"http://localhost:3003/orders\");\n    this.ordData = getData.data;\n  },\n  methods: {\n    format_date(value) {\n      if (value) {\n        const newData = moment(String(value)).format(\"L\");\n        return newData;\n      }\n    },\n  },\n  computed: {\n    allData() {\n      return this.ordData.map((ord) => ({\n        ...ord,\n        emissionDate: this.format_date(ord.emissionDate),\n        value: parseInt(ord.value).toLocaleString(\"pt-br\", {\n          style: \"currency\",\n          currency: \"BRL\",\n        }),\n      }));\n    },\n  },\n};\n</script>\n\n<style>\ndiv {\n  border: 1px solid red;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}